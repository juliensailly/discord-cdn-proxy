{
  "manifest_version": 3,
  "name": "Discord CDN Redirector",
  "version": "1.0",
  "description": "Redirige les requÃªtes de cdn.discordapp.com vers le proxy personnel.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "activeTab"
  ],
  "host_permissions": [
    "*://discord.com/*",
    "*://cdn.discordapp.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}